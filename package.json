    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "parcel-bundler": "^1.12.4",
    "pug": "^3.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.5",
    "workbox-cli": "^5.1.3"
  },
  "dependencies": {
    "crypto-js": "^4.0.0",
